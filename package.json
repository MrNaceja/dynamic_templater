{
  "name": "templater",
  "displayName": "Templater",
  "description": "Make Your Own Dynamic Templates",
  "publisher": "Naceja",
  "license": "MIT",
  "version": "1.0.5",
  "author": {
    "name": "Eduardo Toriani (Naceja)",
    "email": "edutoriani13@gmail.com",
    "url": "https://github.com/MrNaceja"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Snippets",
    "Themes",
    "Keymaps",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "repository": {
    "url": "https://github.com/MrNaceja/templater"
  },
  "contributes": {
    "commands": [
      {
        "command": "templater.newTemplate",
        "title": "Create new Template",
        "category": "Templater"
      },
      {
        "command": "templater.newFile",
        "title": "Create new File based on Template",
        "category": "Templater",
        "enablement": "explorerViewletFocus && explorerViewletVisible"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "templater.newFile",
          "group": "Templater"
        }
      ]
    },
    "configuration": {
      "title": "Templater",
      "properties": {
        "templater.author.name": {
          "type": "string",
          "default": "Author"
        },
        "templater.author.email": {
          "type": "string",
          "default": "author@email.com"
        },
        "templater.customOptions": {
          "type": "object",
          "default": {
            "customValue": "Wowwww is a custom value parameter :)"
          },
          "title": "Custom options to template render",
          "description": "Defines your custom options sended to template render as customOptions in options parameter."
        },
        "templater.templatesDir": {
          "type": "string",
          "default": "",
          "description": "A templates custom directory"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.88.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9"
  }
}
